		"@sveltejs/kit": "^2.49.4",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"svelte-meta-tags": "^4.5.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@tryghost/content-api": "^1.12.2"
